{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "frontendDist": "../dist",
        "devUrl": "http://localhost:8080"
    },
    "bundle": {
        "active": true,
        "category": "DeveloperTool",
        "copyright": "",
        "targets": "all",
        "externalBin": [],
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "windows": {
            "certificateThumbprint": null,
            "digestAlgorithm": "sha256",
            "timestampUrl": ""
        },
        "longDescription": "",
        "macOS": {
            "entitlements": null,
            "exceptionDomain": "",
            "frameworks": [],
            "providerShortName": null,
            "signingIdentity": null
        },
        "resources": [],
        "shortDescription": "",
        "linux": {
            "deb": {
                "depends": []
            }
        }
    },
    "productName": "neptune",
    "mainBinaryName": "neptune",
    "version": "0.1.0",
    "identifier": "com.tauri.dev",
    "plugins": {},
    "app": {
        "windows": [
            {
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "Neptune",
                "width": 800
            }
        ],
        "security": {
            "csp": null,
            "assetProtocol": {
                "enable": true,
                "scope": ["$HOME"]
            }
        }
    }
}
